{
  "name": "port",
  "productName": "Port",
  "version": "1.10.0",
  "description": "A ship runner and manager for Urbit OS",
  "repository": {
    "type": "git",
    "url": "https://github.com/urbit/port.git"
  },
  "keywords": [],
  "author": {
    "name": "Urbit Foundation",
    "email": "dev@urbit.org"
  },
  "license": "MIT",
  "main": ".webpack/main",
  "config": {
    "forge": "./forge.config.js"
  },
  "scripts": {
    "start": "electron-forge start -- --no-sandbox",
    "dev:reset": "rm -rf .webpack && rm -rf node_modules && yarn",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:all": "npm run make:mac && npm run make:mac-arm && npm run make:linux",
    "make:mac": "electron-forge make --platform darwin --arch x64",
    "make:mac-arm": "electron-forge make --platform darwin --arch arm64",
    "make:linux": "DEBUG='electron-forge-maker-snap:*,electron-forge:lifecycle,electron-installer-snap:snapcraft' electron-forge make --platform linux --arch x64",
    "make:snap": "DEBUG='electron-forge-maker-snap:*,electron-forge:lifecycle' electron-forge make --platform linux --arch x64 --targets=@electron-forge/maker-snap",
    "make:win": "electron-forge make --platform win32 --arch x64",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts .",
    "urbit": "./get-urbit.sh",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@davidwinter/electron-forge-maker-snap": "^2.0.3",
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-dmg": "^6.0.0-beta.65",
    "@electron-forge/maker-flatpak": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-snap": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.54",
    "@electron-forge/publisher-github": "^6.0.0-beta.54",
    "@types/async": "^3.2.5",
    "@types/lodash.debounce": "^4.0.6",
    "@types/mv": "^2.1.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "autoprefixer": "^10.2.4",
    "css-loader": "^4.2.1",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "electron": "^18.0.1",
    "electron-installer-dmg": "^4.0.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "fs-extra": "^8.1.0",
    "mini-css-extract-plugin": "^1.3.6",
    "node-loader": "^1.0.1",
    "postcss": "^8.2.6",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.2.0",
    "postcss-preset-env": "^6.7.0",
    "string-replace-loader": "^3.0.3",
    "style-loader": "^1.2.1",
    "tailwindcss": "^2.0.3",
    "ts-loader": "^8.0.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@radix-ui/react-collapsible": "^0.1.6",
    "@radix-ui/react-dialog": "^0.0.16",
    "@radix-ui/react-dropdown-menu": "^0.0.18",
    "@radix-ui/react-popover": "^0.0.16",
    "@radix-ui/react-tabs": "^0.0.12",
    "@radix-ui/react-toggle": "^0.0.10",
    "@radix-ui/react-tooltip": "^0.0.17",
    "@types/adm-zip": "^0.4.33",
    "adm-zip": "^0.5.4",
    "app-root-dir": "^1.0.2",
    "async": "^3.2.0",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "date-fns": "^2.19.0",
    "electron-context-menu": "^2.5.0",
    "electron-is-dev": "^1.2.0",
    "electron-root-path": "^1.0.16",
    "electron-squirrel-startup": "^1.0.0",
    "electron-window-state": "^5.0.3",
    "find-process": "^1.4.4",
    "lodash.debounce": "^4.0.8",
    "mv": "^2.1.1",
    "nedb": "^1.8.0",
    "nedb-async": "^0.1.6",
    "node-ipc": "^9.1.3",
    "node-pty": "^0.10.1",
    "patch-package": "^6.4.7",
    "query-string": "^6.14.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-error-boundary": "^3.1.0",
    "react-hook-form": "^6.15.1",
    "react-hot-loader": "^4.13.0",
    "react-query": "^3.9.2",
    "react-router-dom": "^5.2.0",
    "update-electron-app": "^2.0.1",
    "use-resize-observer": "^7.0.0",
    "xterm-addon-fit": "^0.5.0",
    "xterm-for-react": "^1.0.4",
    "zustand": "^3.3.1"
  },
  "resolutions": {
    "**/node-abi": "^3.8.0"
  }
}
